{
    "editor.fontSize": 18,
    "editor.tabSize": 2,
    "workbench.colorTheme": "Visual Studio Dark",
    "workbench.productIconTheme": "fluent-icons",
    "eslint.experimental.useFlatConfig": true,
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "always",
    },
    "eslint.validate": [
      "javascript",
      "javascriptreact"
    ]
}